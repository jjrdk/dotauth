<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>SimpleAuth Marten Stores</Description>
    <Title>SimpleAuth Marten Stores</Title>
    <TargetFrameworks>net5.0</TargetFrameworks>
    <AssemblyName>simpleauth.stores.marten</AssemblyName>
    <PackageId>simpleauth.stores.marten</PackageId>
    <RootNamespace>SimpleAuth.Stores.Marten</RootNamespace>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|net5.0|AnyCPU'">
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(PackageId).xml</DocumentationFile>
  </PropertyGroup>
  <Import Project="..\..\shared.proj" />
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="5.0.0-preview.6.20305.6" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="5.0.0-preview.6.20305.6" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\simpleauth.shared\simpleauth.shared.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="marten" Version="3.12.2" />
  </ItemGroup>
</Project>
